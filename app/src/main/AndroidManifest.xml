<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.usjt.starke.starkesenhas">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_starke_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_starke_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
        <activity android:name=".controller.GerarSenhaActivity" />
        <activity android:name=".controller.SplashscreenActivity" />
        <activity android:name=".controller.VisualizarSenhaGeradaActivity" />
        <activity android:name=".controller.ListarSenhaActivity" />
        <activity android:name=".controller.SelecionarSenhaActivity" />
        <activity android:name=".controller.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


    </application>

</manifest>